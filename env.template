# Environment Variables for Knowledge Graph System
# Copy this to .env and fill in your actual keys
# NEVER commit .env to git!

# SAM.gov API Key
# Get at: https://sam.gov/content/api
SAM_API_KEY=your_sam_api_key_here

# Neo4j Database
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here
NEO4J_DATABASE=contactsgraphdb

# Anthropic API Key
# Get at: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Collection Settings (optional)
NAICS_CODES=541512,541511,541519
DEFAULT_DAYS_BACK=30
DEFAULT_LIMIT=10
